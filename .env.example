OPENAI_API_KEY=
# OpenAI SDK 支持通过 base url 连接 OpenRouter（https://openrouter.ai/api/v1）
# OPENAI_BASE_URL=https://openrouter.ai/api/v1
#
# OpenRouter 也支持使用单独的 key/base url（可选）
# OPENROUTER_API_KEY=
# OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
#
# 可选：图片模型（OpenAI 默认 gpt-image-1.5；OpenRouter Gemini 图像模型示例：google/gemini-2.5-flash-image）
OPENAI_IMAGE_MODEL=gpt-image-1.5
#
# 可选：OpenRouter 推荐的站点信息（用于统计/归因）
# OPENROUTER_SITE_URL=http://localhost:3000
# OPENROUTER_SITE_NAME=image-editor-website

# Supabase（用于 Google 登录）
# NEXT_PUBLIC_SUPABASE_URL=
# NEXT_PUBLIC_SUPABASE_ANON_KEY=

# Creem（支付）
# CREEM_API_KEY=
# CREEM_WEBHOOK_SECRET=
# CREEM_TEST_MODE=true
#
# Creem 产品 ID（在 Creem Dashboard 里创建产品后填入）
# NEXT_PUBLIC_CREEM_PRODUCT_STARTER_MONTHLY=
# NEXT_PUBLIC_CREEM_PRODUCT_STARTER_YEARLY=
# NEXT_PUBLIC_CREEM_PRODUCT_PRO_MONTHLY=
# NEXT_PUBLIC_CREEM_PRODUCT_PRO_YEARLY=
# NEXT_PUBLIC_CREEM_PRODUCT_TEAM_MONTHLY=
# NEXT_PUBLIC_CREEM_PRODUCT_TEAM_YEARLY=
